{% extends 'base.html.twig' %}

{% block title %}PullRequest{% endblock %}

{% block body %}
    <h1>PullRequest</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ pull_request.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ pull_request.name }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ pull_request.status }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('pull_request_index') }}">back to list</a>

    <a href="{{ path('pull_request_edit', {'id': pull_request.id}) }}">edit</a>

    {{ include('pull_request/_delete_form.html.twig') }}
{% endblock %}
